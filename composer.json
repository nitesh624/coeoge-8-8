{
  "name": "iitbombayweb/iitbdeptwebproject",
  "description": "IIT Bombay Department Website Distribution Project",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3.0",
    "drupal/core": "~8.8.0",
    "drupal-composer/drupal-scaffold": "~2.4.0",
    "szeidler/composer-patches-cli": "~1.0.0",
    "oomphinc/composer-installers-extender": "^1.1",
    "wikimedia/composer-merge-plugin": "^1.4",
    "drupal/console": "~1.0",
    "drush/drush": "^8.0",
    "solarium/solarium": "~4.2",
    "iitbombayweb/iitbdeptweb": "~8.8.0",
    "iitbombayweb/iitbcommon": "~8.8.0",
    "iitbombayweb/iitbextra": "~8.8.0",
    "iitbombayweb/acadcommon": "~8.8.0",
    "ckeditor/liststyle": "4.8.0",
    "ckeditor/tabletoolstoolbar": "^0.0.1",
    "ckeditor/tableselection": "^4.7",
    "ckeditor/tableresize": "^4.7",
    "drupal/ckeditor_liststyle": "^1.4",
    "drupal/ckeditor_tabletoolstoolbar": "^1.0",
    "drupal/ckeditor_tableselection": "^1.0",
    "drupal/ckeditor_tableresize": "^1.0",
    "drupal/fvm": "^2.0",
    "drupal/upgrade_status": "^3.6",
    "drupal/color_field": "^2.4",
    "drupal/editor_advanced_link": "^1.9",
    "drupal/yearonly": "^9.0",
    "drupal/datetimehideseconds": "^1.1",
    "drupal/paragraphs_jquery_ui_accordion": "^1.4",
    "drupal/entity_usage": "^2.0@beta",
    "drupal/draggableviews": "^2.0",
    "drupal/mixitup_views": "^1.12",
    "drupal/transliterate_filenames": "^1.5",
    "drupal/flexslider": "^2.0",
    "drupal/views_contextual_filters_or": "^1.2",
    "drupal/quick_node_clone": "^1.14",
    "bower-asset/flexslider": "~2.7.2",
    "mixitup_views/mixitup": "^3.0",
    "colorbox/colorbox": "^1.0",
    "drupal/viewsreference": "^1.7",
    "iitbombayweb/coeoge_iitb_dj": " dev-master",
    "drupal/entity_browser": "^2.6",
    "drupal/allowed_formats": "^1.3",
    "drupal/colorbox": "^1.7",
    "drupal/entity_browser_table": "^1.2",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/media_entity_browser": "^2.0@alpha",
    "drupal/twig_tweak": "^2.9",
    "drupal/views_bootstrap": "^3.6",
    "drupal/paragraphs": "^1.12",
    "drupal/ctools": "^3.7",
    "drupal/crop": "^2.1",
    "drupal/ds": "^3.12",
    "drupal/entity_reference_revisions": "^1.9",
    "drupal/externalauth": "^1.3",
    "drupal/field_group": "^3.1",
    "drupal/field_permissions": "^1.1",
    "drupal/image_widget_crop": "^2.3",
    "drupal/libraries": "^3.0@beta",
    "drupal/link_attributes": "^1.11",
    "drupal/linked_field": "^1.3",
    "drupal/name": "^1.0@RC",
    "drupal/pathauto": "^1.8",
    "drupal/shs": "^1.0@alpha",
    "drupal/smtp": "^1.0",
    "drupal/spamspan": "^1.2",
    "drupal/token": "^1.9"
  },
  "conflict": {
    "symfony/http-foundation": "3.4.24"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "acadmixdist": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/iitbdeptweb",
      "no-api": true
    },
    "iitbcommonmodules": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/iitbcommon",
      "no-api": true
    },
    "iitbextramodules": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/iitbextra",
      "no-api": true
    },
    "acadcommonmodules": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/acadcommon",
      "no-api": true
    },
    "coeoge_iitb_dj": {
      "type": "git",
      "url": "https://github.com/IITBombayWeb/coeoge_iitb_dj",
      "no-api": true
    },
    "ckeditor-tableselection": {
      "type": "package",
      "package": {
        "name": "ckeditor/tableselection",
        "version": "4.7.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/tableselection/releases/tableselection_4.7.2.zip",
          "type": "zip"
        }
      }
    },
    "ckeditor-tabletoolstoolbar": {
      "type": "package",
      "package": {
        "name": "ckeditor/tabletoolstoolbar",
        "version": "0.0.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/tabletoolstoolbar/releases/tabletoolstoolbar_0.0.1.zip",
          "type": "zip"
        }
      }
    },
    "ckeditor-liststyle": {
      "type": "package",
      "package": {
        "name": "ckeditor/liststyle",
        "version": "4.8.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ckeditor/plugins/liststyle"
        },
        "dist": {
          "url": "https://download.ckeditor.com/liststyle/releases/liststyle_4.8.0.zip",
          "type": "zip"
        }
      }
    },
    "ckeditor-tableresize": {
      "type": "package",
      "package": {
        "name": "ckeditor/tableresize",
        "version": "4.7.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/tableresize/releases/tableresize_4.7.2.zip",
          "type": "zip"
        }
      }
    },
    "mixitup": {
      "type": "package",
      "package": {
        "name": "mixitup_views/mixitup",
        "version": "3.3.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/patrickkunka/mixitup/archive/v3.zip",
          "type": "zip"
        }
      }
    },
    "colorbox": {
      "type": "package",
      "package": {
        "name": "colorbox/colorbox",
        "version": "1.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/jackmoore/colorbox/archive/1.x.zip",
          "type": "zip"
        }
      }
    }
  },
  "config": {
    "process-timeout": 0
  },
  "extra": {
    "merge-plugin": {
      "include": [
        "docroot/profiles/contrib/iitbdeptweb/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-extra": true,
      "merge-extra-deep": true,
      "merge-scripts": true
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "patch/{$name}": [
        "type:drupal-patch"
      ]
    },
    "enable-patching": true,
    "patches-file": "composer.patches.json"
  },
  "scripts": {
    "post-create-project-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-update-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  }
}
